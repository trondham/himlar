$ORIGIN vagrant.iaas.intern.
$TTL    3600
@	IN	SOA	ns01.vagrant.iaas.intern.    root.vagrant.iaas.intern. (
                     2017032201         ; Serial
                           3600         ; Refresh
                           1800         ; Retry
                         604800         ; Expire
                          43200 )       ; Negative Cache TTL

        IN      NS      ns01.vagrant.iaas.intern.
        IN      NS      ns02.vagrant.iaas.intern.


ns01	  	  IN	  A		<%= @ns1_public_addr %>
ns02	  	  IN	  A		<%= @ns2_public_addr %>

dns-master	  IN      CNAME		ns01.vagrant.iaas.intern.
dns-slave	  IN      CNAME		ns02.vagrant.iaas.intern.
